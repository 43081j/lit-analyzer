{
	"compilerOptions": {
		"plugins": [
			{
				"name": "ts-lit-plugin",
				"verbose": true,
				"globalHtmlTags": ["my-external-tag"],
				"globalHtmlAttributes": ["my-attribute"]
			}
		],
		"experimentalDecorators": true,
		"target": "es5",
		"module": "commonjs",
		"allowJs": true,
		"lib": ["esnext", "dom"],
		"strict": true,
		"esModuleInterop": true
	}
}
